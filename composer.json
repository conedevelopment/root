{
    "name": "conedevelopment/root",
    "description": "Root is an admin package for Laravel applications.",
    "keywords": ["laravel", "admin", "dashboard"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Cone Development",
            "email": "hello@conedevelopment.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cone\\Root\\": "src/",
            "Cone\\Root\\Database\\Seeders\\": "database/seeders/",
            "Cone\\Root\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cone\\Root\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-gd": "*",
        "ext-exif": "*",
        "php": "^8.1 || ^8.2",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/laravel": "^10.0",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0",
        "nunomaduro/larastan": "^2.1.6",
        "laravel/pint": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cone\\Root\\RootServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
