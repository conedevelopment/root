{
    "name": "conedevelopment/root",
    "description": "Root is an admin package for Laravel applications.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "admin",
        "dashboard"
    ],
    "authors": [
        {
            "name": "Cone Development",
            "email": "hello@conedevelopment.com"
        }
    ],
    "require": {
        "php": "^8.3.0 || ^8.4.0",
        "ext-exif": "*",
        "ext-gd": "*",
        "conedevelopment/laravel-auth": "^0.1.0",
        "laravel/framework": "^11.31 || ^12.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/laravel": "^12.0",
        "laravel/pint": "^1.6",
        "mockery/mockery": "^1.6",
        "larastan/larastan": "^3.4",
        "phpunit/phpunit": "^11.5.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cone\\Root\\": "src/",
            "Cone\\Root\\Database\\Seeders\\": "database/seeders/",
            "Cone\\Root\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cone\\Root\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2-dev"
        },
        "laravel": {
            "providers": [
                "Cone\\Root\\RootServiceProvider"
            ]
        }
    }
}
